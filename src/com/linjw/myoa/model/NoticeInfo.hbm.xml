<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.linjw.myoa.model">
  <class table="t_NoticeInfo" name="NoticeInfo">
    <id name="id">
      <generator class="native"/>
    </id>
     <property name="content" type="text" length="65536"/>
    <property name="roll"/>
     <property name="postTime" type="timestamp"/>
      <property name="title"/>
      
      <!-- author属性，本类与User的多对一 -->
      <many-to-one name="author" class="User" column="noticeInfoId"></many-to-one>
    
  </class>
</hibernate-mapping>
