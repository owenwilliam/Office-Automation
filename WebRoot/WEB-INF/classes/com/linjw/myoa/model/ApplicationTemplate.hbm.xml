<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.linjw.myoa.model">
  <class table="t_ApplicationTemplate" name="ApplicationTemplate">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="name"/>
    <property name="processDefinition"/>
    <property name="path"/>
    
    <!-- applications属性，本类与Application的一对多 -->
    <set name="applications" cascade="delete">
      <key column="ApplicationTemplateId"></key>
      <one-to-many class="Application" />
    </set>
 
  </class>
</hibernate-mapping>
